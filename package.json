{
    "name": "th-vue-snippets",
    "displayName": "Tellhow Vue 2.x Snippets",
    "author": "ywzhou",
    "publisher": "ywzhou",
    "icon": "img/logo.png",
    "description": "A Tellhow Vue.js 2 Extension",
    "license": "MIT",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.5.0"
    },
    "keywords": [
        "th-vue-snippets",
        "javascript",
        "snippet",
        "vue",
        "vue 2"
    ],
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ywzhou123/th-vue-snippets"
    },
    "contributes": {
        "languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue",
                    "vue"
                ],
                "extensions": [
                    ".vue"
                ]
            }
        ],
        "snippets": [
            {
                "language": "javascriptreact",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "html",
                "path": "./snippets/html.json"
            },
            {
                "language": "vue-html",
                "path": "./snippets/html.json"
            },
            {
                "language": "html",
                "path": "./snippets/vue.json"
            },
            {
                "language": "vue",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "vue",
                "path": "./snippets/html.json"
            },
            {
                "language": "vue",
                "path": "./snippets/vue.json"
            }
        ]
    }
}
