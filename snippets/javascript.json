    "x-init": {
        "prefix": "x-init",
        "body": [
            "export default {",
            "\tnamespaced: true,",
            "\tstate: {",
            "\t\t$1",
            "\t},",
            "\tmutations: {",
            "\t\t$2",
            "\t},",
            "\tactions: {",
            "\t\t$3",
            "\t},",
            "\tgetters: {",
            "\t\t$4",
            "\t}"
        ],
        "description": "vuex 模板"
    },
    "x-mutation": {
        "prefix": "x-mutation",
        "body": [
            "SET_$1(state, data) {",
            "\t$0",
            "}"
        ],
        "description": "vuex mutation"
    },
    "x-action": {
        "prefix": "x-action",
        "body": [
            "$1({ commit, dispatch }, data) {",
            "\t$0",
            "}"
        ],
        "description": "vuex action"
    },
    "x-getter": {
        "prefix": "x-getter",
        "body": [
            "$1: state => state.$0"
        ],
        "description": "vuex getter"
    },
    "x-getter-function": {
        "prefix": "x-getter-function",
        "body": [
            "$1: (state, getters) => (params) => {",
            "\t$0",
            "}"
        ],
        "description": "vuex getter function"
    }


