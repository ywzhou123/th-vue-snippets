    "r-validator": {
        "prefix": "r-validator",
        "body": [
            "{",
            "\tvalidator: (rule, value, callback) => {",
            "\t\tif(value) {",
            "\t\t\tcallback()",
            "\t\t}",
            "\t\tcallback(new Error('验证失败'))",
            "\t},",
            "\ttrigger: ['blur', 'change']",
